@model dynamic

@{
  ViewBag.Title = "Reactor JS";
  Layout = "~/Views/Shared/_Layout.cshtml";
}

@section includes 
{
}

<canvas id="scene" width='650' height='650'>
</canvas>

@section scripts
{
  @Scripts.Render("~/lib/jquery")
  @Scripts.Render("~/lib/underscore")
@*  @Scripts.Render("~/lib/knockout")*@
  @Scripts.Render("~/src/app")

  <script type="text/javascript">
    $(document).ready(function () {
      var app = new App.Application();
      app.init($('#scene'));
      app.start();
    });
  </script>
}
